{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "import": ["./cspell-code.json"],
  "dictionaries": ["en_US", "awsamplify"],
  "dictionaryDefinitions": [
    {
      "name": "awsamplify",
      "description": "dictionary to enforce spelling and casing of AWS terminology",
      "words": [
        "AppSync",
        "CloudWatch",
        "Cognito",
        "DynamoDB",
        "Fargate",
        "Lambda",
        "S3",
        "ampx"
      ],
      "flagWords": [
        "amplify->Amplify",
        "lambda->Lambda",
        "dynamodb->DynamoDB",
        "appsync->AppSync",
        "cognito->Cognito",
        "s3->S3",
        "Userpool->user pool",
        "User Pool->user pool",
        "userpool->user pool",
        "Identity Pool->identity pool",
        "IdentityPool->identity pool",
        "identitypool->identity pool",
        "Cloudwatch->CloudWatch",
        "cloudwatch->CloudWatch",
        "Datasource->Data source",
        "datasource->data source"
      ]
    },
    {
      "name": "awsamplifygen1",
      "description": "dictionary to enforce spelling for legacy documentation",
      "words": []
    }
  ],
  "words": [
    "CRUDL",
    "Codegen",
    "Entra",
    "Geofence",
    "Geofences",
    "Jamba",
    "Jetpack",
    "Logcat",
    "Modelgen",
    "Nuxt",
    "PITR",
    "Pinia",
    "Podfile",
    "Snapcraft",
    "TOTP",
    "Unauth",
    "cocoapods",
    "codegen",
    "composables",
    "downcasting",
    "geofences",
    "geospatial",
    "jose",
    "kotlin",
    "linestring",
    "longblob",
    "longtext",
    "maplibre",
    "maplibregl",
    "mediumblob",
    "mediumint",
    "mediumtext",
    "passwordless",
    "prepper",
    "pubspec",
    "refetches",
    "refreshable",
    "rehype",
    "snapshotted",
    "tinyblob",
    "tinyint",
    "tinytext",
    "unregisters",
    "upvotes",
    "varbinary"
  ],
  "flagWords": ["hte", "full-stack", "Full-stack", "Full-Stack", "sudo"],
  "patterns": [
    {
      "name": "youtube-embed-ids",
      "pattern": "/embedId=\".*\" /"
    },
    {
      "name": "markdown-urls",
      "description": "ignores the URL contents of a markdown URL",
      "pattern": "(?<=\\])\\(([^\\)]+)\\)"
    },
    {
      "name": "src-attribute-urls",
      "description": "matches src attribute value for components",
      "pattern": "src=\"[^\"]*?\""
    },
    // {
    //   "name": "code-block-titles",
    //   "pattern": "```[a-z]+\\s+title=\"[^\"]*\""
    // },

    {
      "name": "markdown-fenced-code-blocks",
      "pattern": "```[\\s\\S]*?```"
    },
    {
      "name": "markdown-inline-code-blocks",
      "pattern": "`[^`]+`"
    }
  ],
  "ignoreRegExpList": [
    "youtube-embed-ids",
    "markdown-urls",
    "src-attribute-urls",
    "markdown-fenced-code-blocks",
    "markdown-inline-code-blocks"
  ],
  "caseSensitive": true
}
