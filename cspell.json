  "flagWords": ["hte", "full-stack", "Full-stack", "Full-Stack", "sudo"],
  "patterns": [
    {
      "name": "youtube-embed-ids",
      "pattern": "/embedId=\".*\" /"
    },
    {
      "name": "markdown-urls",
      "description": "ignores the URL contents of a markdown URL",
      "pattern": "\\[.*?\\]\\(.*?\\)"
    },
    {
      "name": "src-attribute-urls",
      "description": "matches src attribute value for components",
      "pattern": "src=\"[^\"]*?\""
    }
  ],
  "ignoreRegExpList": [
    "youtube-embed-ids",
    "markdown-urls",
    "src-attribute-urls"
  ],
  "caseSensitive": true,
  "overrides": [
    {
      "description": "Override for code blocks",
      "filename": "**/*.mdx",
      "patterns": [
        {
          "name": "markdown-fenced-code-blocks",
          "pattern": "```[\\s\\S]*?```"
        },
        {
          "name": "markdown-inline-code-blocks",
          "pattern": "`[^`]+`"
        }
      ],
      "includeRegExpList": [
        "markdown-fenced-code-blocks",
        "markdown-inline-code-blocks"
      ],
