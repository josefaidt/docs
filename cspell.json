        "Userpool->user pool",
        "User Pool->user pool",
        "userpool->user pool",
        "Identity Pool->identity pool",
        "IdentityPool->identity pool",
        "identitypool->identity pool",
        "Cloudwatch->CloudWatch",
        "cloudwatch->CloudWatch",
        "Datasource->Data source",
        "datasource->data source"
