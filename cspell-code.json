  "dictionaries": ["en_US", "awsamplifycodeblocks"],
  "patterns": [
    {
      "name": "youtube-embed-ids",
      "pattern": "/embedId=\".*\" /"
    },
    {
      "name": "markdown-urls",
      "description": "ignores the URL contents of a markdown URL",
      "pattern": "(?<=\\])\\(([^\\)]+)\\)"
    },
    {
      "name": "src-attribute-urls",
      "description": "matches src attribute value for components",
      "pattern": "src=\"[^\"]*?\""
    },
    {
      "name": "markdown-fenced-code-blocks",
      "pattern": "```[\\s\\S]*?```"
    },
    {
      "name": "markdown-inline-code-blocks",
      "pattern": "`[^`]+`"
    }
  ],
  "includeRegExpList": [
    "markdown-fenced-code-blocks",
    "markdown-inline-code-blocks"
  ],
  "ignoreRegExpList": [
    "youtube-embed-ids",
    "markdown-urls",
    "src-attribute-urls"
  ],
  "caseSensitive": true
}
